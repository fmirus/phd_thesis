@Article{Chen2018a,
  author        = {Tian Qi Chen and Yulia Rubanova and Jesse Bettencourt and David K. Duvenaud},
  title         = {Neural Ordinary Differential Equations},
  journaltitle  = {CoRR},
  year          = {2018},
