@Article{Crawford2016,
  author       = {Crawford, Eric and Gingerich, Matthew and Eliasmith, Chris},
  title        = {Biologically {P}lausible, {H}uman-{S}cale {K}nowledge {R}epresentation},
  journaltitle = {Cognitive Science},
